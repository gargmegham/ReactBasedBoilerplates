{
  "name": "wieldy",
  "version": "1.0.0",
  "description": "Wieldy Admin Template",
  "main": "index.html",
  "scripts": {
    "default-watch-style-sass": "sass --watch default/assets/scss/main.scss:default/assets/css/light-style-1.min.css --style compressed",
    "horizontal-watch-style-sass": "sass --watch horizontal/assets/scss/main.scss:horizontal/assets/css/style.min.css --style compressed",
    "horizontal-dark-watch-style-sass": "sass --watch horizontal-dark/assets/scss/main.scss:horizontal-dark/assets/css/style.min.css --style compressed",
    "horizontal-inside-nav-watch-style-sass": "sass --watch horizontal-inside-nav/assets/scss/main.scss:horizontal-inside-nav/assets/css/style.min.css --style compressed",
    "horizontal-bottom-nav-watch-style-sass": "sass --watch horizontal-bottom-nav/assets/scss/main.scss:horizontal-bottom-nav/assets/css/style.min.css --style compressed",
    "horizontal-top-nav-watch-style-sass": "sass --watch horizontal-top-nav/assets/scss/main.scss:horizontal-top-nav/assets/css/style.min.css --style compressed",
    "back-office-watch-style-sass": "sass --watch back-office/assets/scss/main.scss:back-office/assets/css/style.min.css --style compressed",
    "back-office-mini-watch-style-sass": "sass --watch back-office-mini/assets/scss/main.scss:back-office-mini/assets/css/style.min.css --style compressed",
    "drawer-watch-style-sass": "sass --watch drawer/assets/scss/main.scss:drawer/assets/css/style.min.css --style compressed",
    "crm-watch-style-sass": "sass --watch crm/assets/scss/main.scss:crm/assets/css/style.min.css --style compressed",
    "default-css-prefix": "postcss default/assets/css/*.css --use autoprefixer -d default/assets/css/",
    "horizontal-css-prefix": "postcss horizontal/assets/css/*.css --use autoprefixer -d horizontal/assets/css/",
    "horizontal-dark-css-prefix": "postcss horizontal-dark/assets/css/*.css --use autoprefixer -d horizontal-dark/assets/css/",
    "horizontal-inside-nav-css-prefix": "postcss horizontal-inside-nav/assets/css/*.css --use autoprefixer -d horizontal-inside-nav/assets/css/",
    "horizontal-bottom-nav-css-prefix": "postcss horizontal-bottom-nav/assets/css/*.css --use autoprefixer -d horizontal-bottom-nav/assets/css/",
    "horizontal-top-nav-css-prefix": "postcss horizontal-top-nav/assets/css/*.css --use autoprefixer -d horizontal-top-nav/assets/css/",
    "back-office-css-prefix": "postcss back-office/assets/css/*.css --use autoprefixer -d back-office/assets/css/",
    "back-office-mini-css-prefix": "postcss back-office-mini/assets/css/*.css --use autoprefixer -d back-office-mini/assets/css/",
    "drawer-css-prefix": "postcss drawer/assets/css/*.css --use autoprefixer -d drawer/assets/css/",
    "crm-css-prefix": "postcss crm/assets/css/*.css --use autoprefixer -d crm/assets/css/",
    "style-default": "npm-run-all --parallel default-watch-style-sass default-css-prefix",
    "style-horizontal": "npm-run-all --parallel horizontal-watch-style-sass horizontal-css-prefix",
    "style-horizontal-dark": "npm-run-all --parallel horizontal-dark-watch-style-sass horizontal-dark-css-prefix",
    "style-horizontal-inside-nav": "npm-run-all --parallel horizontal-inside-nav-watch-style-sass horizontal-inside-nav-css-prefix",
    "style-horizontal-bottom-nav": "npm-run-all --parallel horizontal-bottom-nav-watch-style-sass horizontal-bottom-nav-css-prefix",
    "style-horizontal-top-nav": "npm-run-all --parallel horizontal-top-nav-watch-style-sass horizontal-top-nav-css-prefix",
    "style-back-office": "npm-run-all --parallel back-office-watch-style-sass back-office-css-prefix",
    "style-back-office-mini": "npm-run-all --parallel back-office-mini-watch-style-sass back-office-mini-css-prefix",
    "style-drawer": "npm-run-all --parallel drawer-watch-style-sass drawer-css-prefix",
    "style-crm": "npm-run-all --parallel crm-watch-style-sass crm-css-prefix",
    "run-all": "npm-run-all --parallel style-default style-horizontal style-horizontal-dark style-horizontal-inside-nav style-intranet style-back-office style-back-office-mini style-drawer style-crm"
  },
  "keywords": [
    "wieldy",
    "jQuery",
    "Admin",
    "template",
    "html",
    "bootstrap"
  ],
  "author": "G-axon Team",
  "license": "ISC",
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 40",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "amcharts3": "^3.21.13",
    "ammap3": "^3.21.13",
    "bootstrap": "~4.2.1",
    "chart.js": "^2.7.2",
    "chartist": "^0.11.0",
    "codemirror": "^5.39.2",
    "datamaps": "^0.5.9",
    "datatables.net-bs4": "^1.10.19",
    "dropzone": "^5.4.0",
    "flag-icon-css": "^3.0.0",
    "gmaps": "^0.4.24",
    "jquery": "^3.3.1",
    "masonry-layout": "^4.2.1",
    "owl.carousel": "^2.3.4",
    "patternomaly": "^1.3.0",
    "perfect-scrollbar": "^1.4.0",
    "spectrum-colorpicker": "^1.8.0",
    "summernote": "^0.8.10",
    "sweetalert2": "^7.24.4",
    "tempusdominus-bootstrap-4": "^5.0.1",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
